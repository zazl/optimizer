<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>

<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.httpcontexts">
      <httpcontext
            id="org.dojotoolkit.optimizer.samples.httpcontext">
            <resource-mapping path="/"/>
      </httpcontext>
   </extension>
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/*.jsp"
            class="org.eclipse.equinox.jsp.jasper.registry.JSPFactory"
            httpcontextId="org.dojotoolkit.optimizer.samples.httpcontext">
      </servlet>
   </extension>
</plugin>
